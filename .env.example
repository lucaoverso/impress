# CUPS queue name (lpstat -p)
CUPS_PRINTER=HP_LaserJet

# Persistent spool path for uploaded PDFs
SPOOL_DIR=/var/spool/sistema-impress

# SQLite database path (default: sibling folder ../sistema-impress-data/impressao.db)
# DB_PATH=/opt/sistema-impress-data/impressao.db

# Runtime flags
ENABLE_EMBEDDED_WORKER=false
KEEP_SPOOL_FILES=false

# CUPS command behavior
CUPS_LP_COMMAND=lp
CUPS_LP_TIMEOUT_SECONDS=30
